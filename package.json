{
  "name": "deepit",
  "version": "0.1.1",
  "description": "A pragmatic deep-clone library for JavaScript/TypeScript â€” preserves prototypes, descriptors, Maps, Sets, circular refs and optionally clones functions.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist .turbo",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint 'src/**' 'tests/**' --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callmegautam/deepit.git"
  },
  "keywords": [
    "deep-clone",
    "clone",
    "object-clone",
    "typescript",
    "deepit"
  ],
  "author": "Gautam Suthar <iamgautamsuthar@outlook.com> (https://github.com/callmegautam)",
  "license": "MIT",
  "packageManager": "pnpm@10.11.1",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "husky": "^8.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@types/jest": "^29.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=16"
  }
}
